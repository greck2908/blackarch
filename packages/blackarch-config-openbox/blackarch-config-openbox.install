post_install() {
  echo
  echo "  >> To use openbox blackarch config move the following files:"
  echo "      /etc/xdg/openbox-blackarch                      -> /etc/xdg/openbox"
  echo "      /usr/share/themes/blackarch/openbox-3-blackarch -> /usr/share/themes/blackarch/openbox-3"
  echo
}

post_upgrade() {
  post_install
}
